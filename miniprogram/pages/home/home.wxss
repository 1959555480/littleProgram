.star {
  padding: 0 30rpx;
  flex-direction: row;
  justify-content: space-around;
}
checkbox-group {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
checkbox-group view {
  display: flex;
  flex-direction: row;
}
checkbox-group view .item {
  margin-top: 10rpx;
  display: flex;
  justify-content: center;
  align-content: center;
  width: 30vw;
}
.qual {
  margin-top: 20rpx;
  border-top: 1rpx solid rgba(0, 0, 0, 0.5);
  padding: 0 10rpx;
}
.btn {
  position: fixed;
  bottom: 50rpx;
  left: 0;
  right: 0;
  width: 10rpx;
}
